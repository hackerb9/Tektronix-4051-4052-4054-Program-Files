1 REMARK  ****    FLOWCHART PROGRAM PART II    ****2 REMARK    THIS PORTION SEARCHES THE PROGRAM AND MAKES A BRANCH TABLE,3 REMARK  A FOR...TO.. TABLE AND A NEXT.. TABLE.10 INIT 15 PAGE 20 DIM L$(74),F(3,200),N(200),B(500),F$(3000)21 PRINT "

                       FLOWCHART PROGRAM PART II"24 PRINT "


    What is the title of the program? ";26 INPUT T$27 IF LEN(T$)>0 THEN 2928 T$=" "29 PAGE 30 MOVE 35.432,52.81640 PRINT "FLOWCHART PROGRAM Part II running"50 MOVE 51.56,44.36860 PRINT "Please Stand-by"70 F=080 N=090 B=0100 K=0110 C1=0120 C2=0125 C3=0127 F$=""130 FIND 7135 REMARK *** READ IN LINE FROM PROGRAM ***140 ON EOF (0) THEN 580150 INPUT @33:L$160 A=POS(L$," ",2)170 A$=SEG(L$,A+1,2)180 IF A$="GO" OR A$="IF" OR A$="ON" OR A$="FO" OR A$="NE" THEN 375190 IF A$<>"SE" THEN 140192 A$=SEG(L$,A+5,3)194 IF A$<>"KEY" THEN 140196 K=1200 GO TO 140375 B$=SEG(L$,A+1,3)380 IF A$="NE" THEN 550390 IF A$="FO" THEN 520395 IF A$="ON" THEN 420400 A1=POS(L$,"OF",A+1)410 IF A1<>0 THEN 460415 REMARK *** START OF 'GOTO, GOSUB, IF...THEN, ON...THEN...' ***420 A$=SEG(L$,LEN(L$)-5,6)430 C2=C2+1440 B(C2)=VAL(A$)441 IF B$<>"GOS" THEN 140442 B(C2)=-B(C2)450 GO TO 140455 REMARK *** START OF 'GO TO .. OF..., GOSUB .. OF...' ***460 L$=REP("",1,A1)470 A1=POS(L$,",",1)480 C2=C2+1490 B(C2)=VAL(L$)491 IF B$<>"GOS" THEN 500492 B(C2)=-B(C2)500 IF A1=0 THEN 140510 GO TO 460515 REMARK *** START OF 'FOR...T0..' ***520 C3=C3+1525 F(1,C3)=LEN(F$)+1530 F(2,C3)=LEN(L$)535 F$=REP(L$,F(1,C3),0)540 INPUT @33:L$545 F(3,C3)=VAL(L$)546 C2=C2+1547 B(C2)=VAL(L$)548 GO TO 160549 REMARK *** START OF 'NEXT..' ***550 C1=C1+1560 N(C1)=VAL(L$)570 GO TO 140575 REMARK *** START SORT DATA ***580 DIM N(C1 MAX 1),Z(C2 MAX 1)590 C4=1595 C0=C2600 FOR I=1 TO C0610 A=999999620 FOR I1=1 TO C0630 IF ABS(B(I1))>ABS(A) THEN 660640 A=B(I1)650 D=I1660 NEXT I1670 B(D)=999999680 IF I=1 THEN 720690 IF A<>Z(C4-1) THEN 720700 C2=C2-1710 GO TO 740720 Z(C4)=A730 C4=C4+1740 NEXT I745 REMARK *** START TO RECORD DATA ON FILES 5+6 ***750 DIM Z(C2 MAX 1)760 FIND 5770 WRITE @33:C3,LEN(F$),C2771 FOR I1=1 TO 3772 FOR I=1 TO C3773 WRITE @33:F(I1,I)774 NEXT I775 NEXT I1776 IF LEN(F$)<>0 THEN 778777 F$=" "778 WRITE @33:F$810 WRITE @33:Z,T$,K814 CLOSE 815 