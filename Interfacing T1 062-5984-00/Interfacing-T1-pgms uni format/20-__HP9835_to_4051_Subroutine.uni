10   ! 9835-4051 COMS 48820   ON INT #7,15 CALL Tekservice30   CONTROL MASK 7;"00110000"40   CARD ENABLE 750   COM C,E,L,D,K$[80],P,Tekin$[80]60 SET TIMEOUT 7;10070   P=080   K$=""90   C=E=5100  L=2110  D=11120  ON KBD CALL Keyservice130 DISP "MAIN PROGRAM EXECUTING"140  REM150  REM160  REM170  DISP CHR$(13)180  GOTO 130190 END200  SUB Tekservice210  BEEP220  IF TIME OUT(7)>0 THEN 270230  STATUS 7;Stat240  IF BIT(Stat,4) THEN CALL Input250  IF BIT(Stat,5) THEN CALL Output260  GOTO 290270  DISP "TIMEOUT - MESSAGE NOT SENT"280  STOP290  CARD ENABLE 7300  SUBEND310 SUB Clear(S)350  IF S<1 THEN 430360 FOR I=2 TO 10370  PRINT USING "#,K";CHR$(2)&"H"380  PRINT LIN(I);SPA(80)390  NEXT I400  L=2420  SUBEXIT430  FOR I=11 TO 20440  PRINT USING "#,K";CHR$(27)&"H"450  PRINT LIN(I);SPA(80)460  NEXT I470  D=11490  SUBEND500  SUB Input510  COM C,E,L,D,K$[80],P,Tekin$[80]520  ENTER 7;Tekin$530  PRINT USING "#,K";CHR$(27)&"H"550  PRINT LIN(L);Tekin$580  L=L+1600  IF L>10 THEN CALL Clear(1)610  SUBEND620  SUB Output630  COM C,E,L,D,K$[80],P640  OUTPUT 7;K$650  P=0660  DISP CHR$(12)670  PRINT USING "#,K";CHR$(27)&"H"671  PRINT SPA(80)675  PRINT USING "#,K";CHR$(27)&"H"680  PRINT LIN(D);K$690  K$=""700  D=D+1710  IF D>20 THEN CALL Clear(0)720  SUBEND730  SUB Keyservice740 COM C,E,L,D,K$[80],P750  P=P+1760  K$[P,P]=KBD$770  PRINT USING "#,K";CHR$(27)&"H"780  PRINT SPA(80)785  PRINT USING "#,K";CHR$(27)&"H"790  PRINT K$791  IF LEN(K$)>79 THEN 810800  IF NUM(K$[P,P])<>255 THEN SUBEXIT810  REQUEST 7;64820  SUBEND