1 REM  ******** SOLVE SYSTEM(S) OF REAL LINEAR EQUATIONS ********2 REM  ************** 3 REM  DATA ENTRY 4 GOSUB 10205 END 22 REM  ************** 23 REM  SOLVE SYSTEM 24 GOSUB 154025 END 42 REM  ************** 43 REM  CORRECT MATRIX 44 GO TO 128062 REM  ************** 63 REM  PRINT MATRIX 64 GOSUB 140065 END 66 REM 1000 REM  *********************************************************1010 REM  **************DATA ENTRY ROUTINE*************************1020 PAGE 1030 SET NOKEY 1040 PRINT "ENTER NUMBER OF EQUATIONS (ROWS OF THE MATRIX) = ";1050 INPUT Q11060 PRINT "
ENTER THE NUMBER OF CONSTANT VECTORS YOU HAVE  = ";1070 INPUT Q21080 Q1=INT(Q1)1090 Q2=INT(Q2)1100 Q3=Q1+Q21110 DELETE P1120 DIM P(Q1,Q3)1130 PRINT "
ENTER THE ENTIRE MATRIX BEFORE USING ANY OTHER KEYS."1140 PRINT 1150 FOR P1=1 TO Q11160 PRINT "
ROW #";P11170 FOR P2=1 TO Q31180 PRINT "P(";P1;",";P2;") = ";1190 INPUT P(P1,P2)1200 NEXT P21210 NEXT P11220 SET KEY 1230 PRINT 1240 PRINT "ENTRY FINISHED

"1250 RETURN 1260 REM  ********************************************************** 1270 REM  *****************CORRECT MATRIX ROUTINE******************* 1280 PRINT 1290 PRINT "CORRECT THE MATRIX:  ENTER THE ROW AND COLUMN"1300 PRINT "             OF THE ELEMENT TO BE CORRECTED."1310 PRINT 1320 PRINT "ROW, COLUMN = ";1330 INPUT P1,P21340 PRINT "PRESENT VALUE   = ";P(P1,P2)1350 PRINT "ENTER NEW VALUE = ";1360 INPUT P(P1,P2)1370 GO TO 13101380 REM  ********************************************************** 1390 REM  ************PRINT MATRIX ROUTINE**************************1400 PAGE 1410 PRINT "P MATRIX, PRINTED OUT BY ROWS"1420 FOR P1=1 TO Q11430 PRINT 1440 PRINT "***ROW";P1;"***"1450 FOR P2=1 TO Q31460 PRINT P(P1,P2),"";1470 NEXT P21480 PRINT 1490 NEXT P11500 PRINT "
"1510 RETURN 1520 REM  **********************************************************1530 REM  *******************SOLVE SYSTEM ROUTINE*******************1540 PAGE 1550 GOSUB 19001560 IF Q2=0 THEN 16601570 PRINT "SOLUTION VECTOR(S)"1580 FOR P2=Q1+1 TO Q31590 PRINT 1600 PRINT "VECTOR ";P2-Q11610 FOR P1=1 TO Q11620 PRINT P(P1,P2),"";1630 NEXT P11640 PRINT 1650 NEXT P21660 PRINT 1670 PRINT "DET(A) = ";Q41680 PRINT 1690 PRINT 1700 DIM P$(1)1710 PRINT "PRINT INVERSE OF YOUR MATRIX: ";1720 INPUT P$1730 IF P$="Y" THEN 17601740 IF P$<>"N" THEN 17101750 RETURN 1760 PRINT 1770 PRINT "INVERSE, PRINTED OUT BY ROWS"1780 FOR P1=1 TO Q11790 PRINT 1800 PRINT "***ROW";P1;"***"1810 FOR P2=1 TO Q11820 PRINT P(P1,P2),"";1830 NEXT P21840 PRINT 1850 NEXT P11860 PRINT "
"1870 RETURN 1880 REM  **********************************************************1890 REM  ********GAUSSIAN ELIMINATION WITH COMPLETE PIVOTING*******1900 DELETE Q,P31910 DIM Q(Q1),P3(Q1,2)1920 REM 1930 REM  *****INITIALIZE******1940 REM 1950 Q4=11960 Q=01970 FOR P7=1 TO Q11980 REM1990 REM  *****SEARCH FOR PIVOT ELEMENT****** 2000 REM2010 P6=02020 FOR P1=1 TO Q12030 IF Q(P1)=1 THEN 21202040 FOR P2=1 TO Q12050 IF Q(P2)=1 THEN 21102060 IF Q(P2)>1 THEN 26402070 IF P6>ABS(P(P1,P2)) THEN 21102080 P8=P12090 P9=P22100 P6=ABS(P(P1,P2))2110 NEXT P22120 NEXT P12130 Q(P9)=Q(P9)+12140 REM2150 REM  ******INTERCHANGE ROW TO PUT PIVOT ON DIAGONAL***** 2160 REM2170 IF P8=P9 THEN 22402180 Q4=-Q42190 FOR Q9=1 TO Q32200 Q8=P(P8,Q9)2210 P(P8,Q9)=P(P9,Q9)2220 P(P9,Q9)=Q82230 NEXT Q92240 P3(P7,1)=P82250 P3(P7,2)=P92260 P5=P(P9,P9)2270 IF P5=0 THEN 26702280 Q4=Q4*P52290 P5=1/P52300 REM2310 REM  *****DIVIDE PIVOT ROW BY PIVOT******2320 REM2330 P(P9,P9)=12340 FOR Q9=1 TO Q32350 P(P9,Q9)=P(P9,Q9)*P52360 NEXT Q92370 REM2380 REM  *****REDUCE NON-PIVOT ROWS******2390 REM2400 FOR Q7=1 TO Q12410 IF Q7=P9 THEN 24702420 Q6=P(Q7,P9)2430 P(Q7,P9)=02440 FOR Q9=1 TO Q32450 P(Q7,Q9)=P(Q7,Q9)-P(P9,Q9)*Q62460 NEXT Q92470 NEXT Q72480 NEXT P72490 REM 2500 REM  *****INTERCHANGE COLUMNS*****2510 REM 2520 FOR Q9=Q1 TO 1 STEP -12530 IF P3(Q9,1)=P3(Q9,2) THEN 26102540 Q5=P3(Q9,1)2550 P4=P3(Q9,2)2560 FOR P2=1 TO Q12570 Q8=P(P2,Q5)2580 P(P2,Q5)=P(P2,P4)2590 P(P2,P4)=Q82600 NEXT P22610 NEXT Q92620 DELETE Q,P32630 RETURN 2640 PRINT 2650 PRINT "ERROR IN SOLVE SYSTEM ROUTINE"2660 END 2670 PRINT 2680 PRINT "INVERSE DOES NOT EXIST AND DET = 0"2690 END 2700 REM  ***************END OF SOLVE SYSTEM ROUTINE**************** 2710 REM  ********************************************************** 2720 REM 