190 END 200 REM  INVERSE OF COMPLEX TRIG FUNCTIONS (REGULAR AND HYPERBOLIC) *205 REM  REFN:  A. ABRAMMOWITZ AND I.A. STEGUN (EDITORS), 210 REM         "HANDBOOK OF MATHEMATICAL FUNCTIONS", AMS 55, 215 REM         DEPT. OF COMMERCE, WASHINGTON, D.C., 1964.220 REM  Z = (X,Y) IN RECTANGULAR COORDINATES 225 REM  Z2 = ARCSIN(Z1), ETC.230 REM 235 REM 240 REM 245 REM 250 REM  Z2 = ARCSIN(Z1)  **********255 X2=(X1+1)^2+Y1^2260 Y2=SQR(X2-4*X1)265 X2=SQR(X2)270 P8=0.5*(X2+Y2)275 P9=0.5*(X2-Y2)280 X2=ASN(P9)285 Y2=LOG(P8+SQR(P8^2-1))290 RETURN 295 REM   END ARCSIN  **********300 REM 305 REM  Z2 = ARCSINH(Z1) = -*ARCSN(I*Z1)  *R(********* 310 X2=(1-Y1)^2+X1^2315 Y2=SQR(X2+4*Y1)320 X2=SQR(X2)325 P8=0.5*(X2+Y2)330 P9=0.5*(X2-Y2)335 Y2=-ASN(P9)340 X2=LOG(P8+SQR(P8^2-1))345 RETURN 350 REM   END ARCSINH **********355 REM 360 REM 365 REM 370 REM 375 REM 380 REM 385 REM 390 REM 395 REM 400 REM  Z2 = ARCCOS(Z1)  **********405 X2=(X1+1)^2+Y1^2410 Y2=SQR(X2-4*X1)415 X2=SQR(X2)420 P8=0.5*(X2+Y2)425 P9=0.5*(X2-Y2)430 X2=ACS(P9)435 Y2=-LOG(P8+SQR(P8^2-1))440 RETURN 445 REM   END ARCCOS  **********450 REM 455 REM  Z2 = ARCCOSH(Z1) = I*ARCCOS(Z1)  **********460 GOSUB 400465 P9=X2470 X2=-Y2475 Y2=P9480 RETURN 485 REM   END ARCCOSH **********490 REM 495 REM 500 REM  Z2 = ARCTAN(Z1)  **********505 Y2=X1^2+Y1^2510 IF Y2<>1 THEN 540515 IF ABS(Y1)<>1 THEN 530520 PRINT "ERROR IN ARCTAN, NOT DEFINED FOR Z1 = I OR -I"525 END 530 X2=PI/4535 GO TO 545540 X2=0.5*ATN(2*X1/(1-Y2))545 Y2=0.25*LOG((Y2+2*Y1+1)/(Y2-2*Y1+1))550 RETURN 555 REM   END ARCTAN ********** 560 REM 565 REM 570 REM 575 REM 580 REM 585 REM 590 REM 595 REM 600 REM  Z2 = ARCTANH(Z1) = I*ARCTAN(-I*Z1)  ***********605 X2=X1^2+Y1^2610 IF X2<>1 THEN 640615 IF ABS(X1)<>1 THEN 630620 PRINT "ERROR IN ARCTANH, NOT DEFINED FOR Z1 = 1 OR -1"625 END 630 Y2=PI/4635 GO TO 645640 Y2=0.5*ATN(2*Y1/(1-X2))645 X2=-0.25*LOG((X2-2*X1+1)/(X2+2*X1+1))650 RETURN 655 REM   END ARCTANH **********660 REM 