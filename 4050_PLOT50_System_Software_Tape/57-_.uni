200 PAGE 206 N6=1207 N7=1220 PRINT "                   AN X vs Y DATA PLOTThe X vs Y ";230 PRINT "Data Plot stores a series of numbers specified by the ";240 PRINT "user,and plots these numbers as Y coordinate values ";250 PRINT "with respect to Xcoordinate values."260 PRINT "  DATA VALUES   X      Y"270 DIM A(20,2)280 FOR I=1 TO 20290 A(I,1)=RND(-1)*100300 A(I,2)=RND(-1)*100310 PRINT USING "2(4D.2D)":A(I,1),A(I,2)320 NEXT I330 VIEWPORT 50,120,10,80340 WINDOW 0,100,0,100350 MOVE 0,0360 DRAW 100,0370 DRAW 100,100380 DRAW 0,100390 DRAW 0,0400 X=50410 Y=10420 Z=0430 PRINT @32,20:X-1,Y440 PRINT @32,21:X-10,Y-1.4450 PRINT USING "3D":Z460 FOR I=1 TO 10470 PRINT @32,21:X,Y480 Y=Y+7490 PRINT @32,20:X,Y,X-1,Y500 PRINT @32,21:X-10,Y-1.4510 Z=Z+10520 PRINT USING "3D":Z530 NEXT I540 Y=10550 PRINT @32,21:X,Y560 Z=0570 PRINT @32,20:X,Y-1580 PRINT @32,21:X-3,Y-4590 PRINT USING "3D":Z600 FOR I=1 TO 10610 PRINT @32,21:X,Y620 X=X+7630 PRINT @32,20:X,Y,X,Y-1640 PRINT @32,21:X-3,Y-4650 Z=Z+10660 PRINT USING "3D":Z670 NEXT I680 FOR I=1 TO 20690 MOVE A(I,1),A(I,2)700 Z=1.3710 RMOVE 0,Z720 RDRAW Z,-Z730 RDRAW -Z,-Z740 RDRAW -Z,Z750 RDRAW Z,Z760 RMOVE 0,-Z770 NEXT I780 PRINT @32,21:0,6790 PRINT "Would you like to see another X,Y data plot?(YES,NO)";800 GOSUB 150810 GO TO K0 OF 830,200,820,175,156,163,156,181,179820 GO TO 107830 PAGE 840 GO TO 790