100 INIT 110 PAGE 120 DIM S$(24500),T$(3000),S(5),T(5),X(5),Y(5),F(5)130 DATA 0,0,0,0.5,0.66,0,0,0,0.5,0.68,68.5,70,10,33,62135 DATA 64,75,0,-28,-28,24,22,20,14,18140 READ S,T,X,Y,F150 FOR I=1 TO 5160 S$=""170 FIND @5:F(I)180 READ @5:T$190 IF T$="|EOG|" THEN 220200 S$=S$&T$210 GO TO 180220 IF I=1 OR I=2 OR I=3 THEN 240230 CALL "RSCALE",S$,S(I),T(I),0,0240 CALL "RDRAW",S$,1,X(I),Y(I)250 NEXT I260 END 