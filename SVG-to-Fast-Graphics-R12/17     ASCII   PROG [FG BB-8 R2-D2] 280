100 INIT 110 PAGE 120 DIM S$(24500),T$(3000),S(4),T(4),X(4),Y(4),F(4)130 DATA 0,0,0.5,0.66,0,0,0.5,0.66,70,10,33,62,75,0,-28,-28,22,20,14,18140 READ S,T,X,Y,F150 FOR I=1 TO 4160 S$=""170 FIND @5:F(I)180 READ @5:T$190 IF T$="|EOG|" THEN 220200 S$=S$&T$210 GO TO 180220 IF I=1 OR I=2 THEN 240230 CALL "RSCALE",S$,S(I),T(I),0,0240 CALL "RDRAW",S$,1,X(I),Y(I)250 NEXT I260 END 