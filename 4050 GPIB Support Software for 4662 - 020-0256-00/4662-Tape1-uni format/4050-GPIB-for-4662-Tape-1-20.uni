100 REM FILE 20: PICTURE INDEX MAPPING110 REM Part #062-3409-00120 REM 4O5O SERIES GPIB SUPPORT SOFTWARE FOR 4662130 INIT140 SET DEGREES150 PAGE160 PRINT "	PICTURE INDEX MAPPING"170 DIM X$(40),A$(40),D1(4)180 T=1190 PRINT @T,17:1.4,1.4200 PRINT "
Insert STORAGE TAPE and enter file number ";210 INPUT A$220 F=INT(VAL(A$))230 N1=0240 FIND F250 READ @33:X$,O4,X,Z260 READ @33:X270 N1=N1+1280 IF X<800000 THEN 260290 FIND F300 READ @33:X$,O4,X,Z310 GOSUB 700320 D1(1)=X330 D1(2)=Y340 X=Z350 GOSUB 700360 D1(3)=X370 D1(4)=Y380 PRINT "
