1 REM ** MENU DRAWING FOR 4662 PLOTTER OUTPUT.  **2 REM ** WRITTEN BY TOM SUTHERLIN CAMERON UNIVERSITY JAN. 1977 **3 REM ** 51/00-9543/0 **4 GO TO 100100 INIT 110 SET DEGREES 120 WINDOW 0,1500,0,1000130 VIEWPORT 0,1500,0,1000140 SCALE 0.1,0.1150 MOVE @1:15,10160 DRAW @1:15,0170 DRAW @1:0,0180 DRAW @1:0,10190 DRAW @1:15,10200 MOVE @1:14,10210 DRAW @1:14,0220 MOVE @1:13,0230 DRAW @1:13,10240 MOVE @1:13,9250 DRAW @1:15,9260 MOVE @1:15,8270 DRAW @1:13,8280 MOVE @1:13,7290 DRAW @1:15,7300 MOVE @1:15,6310 DRAW @1:13,6320 MOVE @1:13,5330 DRAW @1:15,5340 MOVE @1:15,4350 DRAW @1:13,4360 MOVE @1:13,3370 DRAW @1:15,3380 MOVE @1:15,2390 DRAW @1:13,2400 MOVE @1:13,1410 DRAW @1:15,1420 PRINT @1,17:1,1.5430 REM LINE440 MOVE @1:13.1,9.1450 DRAW @1:13.9,9.9460 MOVE @1:13.1,9.8470 PRINT @1:"Line";480 REM HIDDEN LINE490 MOVE @1:13.1,8.1500 ROTATE 45510 FOR I=1 TO 5520 RDRAW @1:0.1,0530 RMOVE @1:0.1,0540 NEXT I550 RDRAW @1:0.1,0560 ROTATE 0570 MOVE @1:13.1,8.8580 PRINT @1:"Hidden"590 MOVE @1:13.1,8.65600 PRINT @1:"Line";610 REM CENTER LINE620 MOVE @1:13.1,7.1630 ROTATE 45640 RDRAW @1:0.4,0650 RMOVE @1:0.1,0660 RDRAW @1:0.1,0670 RMOVE @1:0.1,0680 RDRAW @1:0.4,0690 ROTATE 0700 MOVE @1:13.1,7.8710 PRINT @1:"Center"720 MOVE @1:13.1,7.65730 PRINT @1:"Line";740 REM CIRCLE750 MOVE @1:13.5,6.5760 GIN @1:A,B770 RMOVE @1:0.4,0780 FOR I=10 TO 360 STEP 10790 DRAW @1:0.4*COS(I)+A,0.4*SIN(I)+B800 NEXT I810 MOVE @1:13.2,6.5820 PRINT @1:"Circle";830 REM HIDDEN CIRCLE840 MOVE @1:13.5,5.5850 GIN @1:A,B860 RMOVE @1:0.4,0870 FOR I=10 TO 360 STEP 10880 DRAW @1:0.4*COS(I)+A,0.4*SIN(I)+B890 I=I+10900 MOVE @1:0.4*COS(I)+A,0.4*SIN(I)+B910 NEXT I920 MOVE @1:13.2,5.5930 PRINT @1:"Hidden"940 MOVE @1:13.2,5.35950 PRINT @1:"Circle";960 REM PARTIAL ARC970 MOVE @1:13.5,4.5980 GIN @1:A,B990 RMOVE @1:0.4,01000 FOR I=10 TO 120 STEP 101010 DRAW @1:0.4*COS(I)+A,0.4*SIN(I)+B1020 NEXT I1030 MOVE @1:13.1,4.61040 PRINT @1:"Partial"1050 MOVE @1:13.1,4.451060 PRINT @1:"Arc";1070 REM PARTIAL HIDDEN ARC1080 MOVE @1:13.5,3.51090 GIN @1:A,B1100 RMOVE @1:0.4,01110 FOR I=10 TO 120 STEP 101120 DRAW @1:0.4*COS(I)+A,0.4*SIN(I)+B1130 I=I+101140 MOVE @1:0.4*COS(I)+A,0.4*SIN(I)+B1150 NEXT I1160 MOVE @1:13.1,3.61170 PRINT @1:"Partial"1180 MOVE @1:13.1,3.451190 PRINT @1:"Hidden"1200 MOVE @1:13.1,3.31210 PRINT @1:"Arc";1220 REM LEADER1230 MOVE @1:13.1,2.11240 ROTATE 451250 RDRAW @1:0.8,01260 RDRAW @1:0,-0.051270 RDRAW @1:0.1,0.051280 RDRAW @1:-0.1,0.051290 RDRAW @1:0,-0.051300 ROTATE 01310 MOVE @1:13.1,2.81320 PRINT @1:"Leader";1330 REM DIMENSION CALCULATION1340 MOVE @1:13.05,1.51350 PRINT @1:"Dimension"1360 MOVE @1:13.3,1.351370 PRINT @1:"Input";1380 MOVE @1:13.35,0.71390 PRINT @1:"END"1400 MOVE @1:13.4,0.51410 PRINT @1:"of"1420 MOVE @1:13.15,0.21430 PRINT @1:"PROGRAM"1440 MOVE @1:14.2,9.31450 DRAW @1:14.8,9.31460 DRAW @1:14.8,9.71470 DRAW @1:14.2,9.71480 DRAW @1:14.2,9.31490 MOVE @1:14.05,9.11500 PRINT @1:"Rectangle"1510 MOVE @1:14.25,8.71520 PRINT @1:"POINT"1530 MOVE @1:14.4,8.51540 PRINT @1:"to"1550 MOVE @1:14.25,8.31560 PRINT @1:"POINT"1570 MOVE @1:14.25,7.71580 PRINT @1:"CROSS";1590 MOVE @1:14.25,7.41600 PRINT @1:"HATCH";1610 MOVE @1:15,101620 END 