100 REM ************** COMPRESS PDB110 PRINT "DO NOT INTERRUPT!"120 READ #1,1:I5130 I0=I5140 IF I5<1 THEN 550150 I9=I5160 READ #1,I9:C1,L0170 PRINT I9;" ";180 IF L0<>0 THEN 480190 I0=I0-1200 FOR I8=I9 TO I5210 READ #1,I8+1:C1,L0220 IF C1=1 THEN 290230 IF C1=2 THEN 320240 IF C1=3 THEN 350250 IF C1=4 THEN 380260 IF C1=5 THEN 410270 IF C1=6 THEN 440280 GO TO 470290 READ #1,I8+1:C1,L0,D2,X1,Y1,X2,Y2300 WRITE #1,I8:C1,L0,D2,X1,Y1,X2,Y2310 GO TO 470320 READ #1,I8+1:C1,L0,X1,Y1,A2,H2,J2,C$330 WRITE #1,I8:C1,L0,X1,Y1,A2,H2,J2,C$340 GO TO 470350 READ #1,I8+1:C1,L0,N1,A3,D3,P360 WRITE #1,I8:C1,L0,N1,A3,D3,P370 GO TO 470380 READ #1,I8+1:C1,L0,X3,Y3,R3,S3,F$390 WRITE #1,I8:C1,L0,X3,Y3,R3,S3,F$400 GO TO 470410 READ #1,I8+1:C1,L0,X1,Y1,X2,Y2,A6,A4420 WRITE #1,I8:C1,L0,X1,Y1,X2,Y2,A6,A4430 GO TO 470440 READ #1,I8+1:C1,L0,X1,Y1,R1,B1,B2450 WRITE #1,I8:C1,L0,X1,Y1,R1,B1,B2460 GO TO 470470 NEXT I8480 I9=I9-1490 IF I9>1 THEN 160500 WRITE #1,1:I0510 P5=I0+1520 PRINT 530 PRINT "
Compress Complete!"540 INPUT Z$550 RETURN 