100 REM *** DATA ALIGNMENT ***110 REM *** S.K.SANFORD / MARCH 1977 ***120 INIT 130 REM *** U0 = OUTPUT DATA UNIT ***140 U0=2150 ON SRQ THEN 180160 PRINT "	*** DATA ALIGNMENT ***J_J_"170 GO TO 210180 INPUT @U0,30:T0190 IF T0<>12 THEN 1120200 GO TO 930210 PRINT "J_	***************************************************"220 PRINT "	* Y IS ASSUMED TO BE A STRICT FUNCTION OF X       *"230 PRINT "	* X(SAMPLE N+1) MUST BE > X(SAMPLE N)             *"240 PRINT "	* ANY X(SAMPLE N+1) < OR = X(SAMPLE N) IS IGNORED *"250 PRINT "	***************************************************"260 FIND 0270 PRINT "J_INSERT INPUT TAPE INTO INTERNAL TAPE DRIVE"280 PRINT "J_PRESS  RETURN  WHEN READY";290 INPUT R$300 PRINT "J_ENTER INPUT FILE NUMBER: ";310 INPUT R320 IF R<1 OR R>255 THEN 300330 FIND R340 PRINT "J_ENTER STARTING VALUE: ";350 INPUT I1360 PRINT "ENTER INCREMENT: ";370 INPUT I2380 PRINT "ENTER ENDING VALUE: ";390 INPUT I3400 M=1410 PRINT "J_STORE OUTPUT DATA ON TAPE? (Y/N): ";420 INPUT R$430 IF R$="N" THEN 590440 IF R$<>"Y" THEN 410450 PRINT "J_PLACE OUTPUT TAPE INTO TAPE UNIT ";U0460 PRINT "J_PRESS  RETURN  WHEN READY"470 INPUT R$480 M=2490 PRINT "J_J_ENTER OUTPUT FILE NUMBER: ";500 INPUT R510 FIND @U0:R520 PRINT "J_IS FILE PRE-MARKED? (Y/N): ";530 INPUT R$540 IF R$="Y" THEN 590550 IF R$<>"N" THEN 520560 MARK @U0:1,50000570 FIND @U0:R580 PRINT "J_FILE IS MARKED FOR 50000 BYTES"590 L=1600 PRINT "J_PROOF-READ OUTPUT DATA? (Y/N): ";610 INPUT R$620 IF R$="N" THEN 720630 IF R$<>"Y" THEN 600640 L=2650 PRINT @32,26:0660 PRINT "J_AUTOMATIC COPY? (Y/N): ";670 INPUT R$680 IF R$="N" THEN 710690 IF R$<>"Y" THEN 660700 PRINT @32,26:3710 PRINT "     X	     YJ_"720 ON EOF (0) THEN 930730 INPUT @33:X1,Y1740 INPUT @33:X,Y750 FOR I=I1 TO I3 STEP I2760 GO TO 800770 X1=X780 Y1=Y790 INPUT @33:X,Y800 IF X1>I THEN 920810 IF X<=I THEN 770820 IF X<=X1 THEN 790830 F=(I-X1)/(X-X1)840 Z=(Y-Y1)*F+Y1850 GO TO 870860 Z=Y1870 W=I880 GO TO M OF 900,890890 PRINT @U0:W,Z900 GO TO L OF 920,910910 PRINT W,Z920 NEXT I930 GO TO M OF 950,940940 FIND @U0:R950 PRINT "J_J_ *** DONE ***"960 GO TO L OF 1000,970970 MOVE 0,0980 PRINT 990 PRINT @32,26:01000 PRINT "ANOTHER RUN? (Y/N): ";1010 INPUT R$1020 IF R$="Y" THEN 1001030 IF R$<>"N" THEN 10001040 FIND 01050 PRINT "J_REPLACE PROGRAM TAPE INTO INTERNAL TAPE DRIVE"1060 PRINT "J_PRESS  RETURN  WHEN READY";1070 INPUT R$1080 REM *** FOR PROGRAM TAPES WITH DIRECTORY PROGRAM ON FILE 1 ***1090 FIND 11100 OLD 1110 END 1120 PRINT " **** TAPE ERROR CODE ";T0;" ****J_J_J_"1130 END 