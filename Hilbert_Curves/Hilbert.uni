1 REM File 17 -- hilbert curves88 IMAGE "Time=",3D.1D," secs"100 FOR _order=3 to 6110 REM CALL "!time",A0,B0,C0,D0120 WINDOW 0,130,0,100130 PAGE140 MOVE 110,95150 PRINT "ORDER =";_order160 WINDOW 0,1.3*2^_order,0,2^_order170 H=1  ! length of side180 N=1  ! north190 S=2  ! south200 E=3  ! east210 W=4  ! west220 X=0230 Y=0240 MOVE X,Y250 CALL Hilbert(E,N,W,S,_order)260 REM CALL "!time",A1,B1,C1,D1270 REM S0=A0*3600+B0*60+C0+D0/100280 REM S1=A1*3600+B1*60+C1+D1/100290 REM S=S1-S0300 WINDOW 0,130,0,100310 MOVE 100,90320 PRINT USING 50:S330 REM CALL "!pause",4340 NEXT _order350 END360 SUB Hilbert(R,D,L,U,I)370 IF I>0 THEN380 CALL Hilbert(D,R,U,L,I-1)390 CALL _move(R,R,H)400 CALL Hilbert(R,D,L,U,I-1)410 CALL _move(D,D,H)420 CALL Hilbert(R,D,L,U,I-1)430 CALL _move(L,L,H)440 CALL Hilbert(U,L,D,R,I-1)450 END IF460 END SUB470 SUB _move(Xx,Yy,H)480 GOSUB Xx of 530,550,570,590490 IF Xx=Yy THEN 510500 GOSUB Yy of 530,550,570,590510 DRAW X,Y520 END SUB530 Y=Y+H  ! north540 RETURN550 Y=Y-H  ! south560 RETURN570 X=X+H  ! east580 RETURN590 X=X-H  ! west600 RETURN